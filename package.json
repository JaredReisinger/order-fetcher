{
  "name": "order-fetcher",
  "version": "5.0.1",
  "author": "Jared Reisinger",
  "license": "ISC",
  "description": "WooCommerce order fetcher",
  "keywords": [
    "WooCommerce",
    "woocommerce",
    "orders",
    "csv"
  ],
  "homepage": "https://github.com/JaredReisinger/order-fetcher#readme",
  "repository": "github:JaredReisinger/order-fetcher",
  "type": "module",
  "files": [
    "dist/**/*.js",
    "!dist/**/*.test.js"
  ],
  "bin": {
    "orders": "./dist/stub.js",
    "order-fetcher": "./dist/stub.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node --import=tsimp/import src/stub.ts",
    "clean": "rm -r .tsimp coverage dist",
    "lint": "eslint ./src",
    "test": "ava --config .ava.tsimp.config.js",
    "test:coverage": "c8 --all --src src --exclude 'src/stub.ts' --exclude 'src/wc/woocommerce-types.ts' --per-file --reporter lcov --reporter text npm run test",
    "test:dist": "npm run build && npx ava --config .ava.dist.config.js",
    "test:dist:coverage": "c8 --all --src src --exclude 'src/stub.ts' --exclude 'src/wc/woocommerce-types.ts' --per-file --reporter lcov --reporter text npm run test:dist",
    "coverage": "npm run test:dist:coverage",
    "commit": "git-cz",
    "release": "semantic-release",
    "snyk-protect": "snyk-protect",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@inquirer/core": "^9.0.3",
    "@inquirer/prompts": "^5.2.0",
    "@json2csv/plainjs": "^7.0.6",
    "@snyk/protect": "^1.1292.1",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "he": "^1.2.0",
    "ky": "^1.4.0",
    "lodash.get": "^4.4.2",
    "moment-timezone": "^0.5.45",
    "parse-link-header": "^2.0.0",
    "printable-characters": "^1.0.42",
    "read-package-up": "^11.0.0",
    "table": "^6.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@inquirer/testing": "^2.1.27",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint__js": "^8.42.3",
    "@types/he": "^1.2.3",
    "@types/lodash.get": "^4.4.9",
    "@types/node": "^20.14.11",
    "@types/parse-link-header": "^2.0.3",
    "@types/sinon": "^17.0.3",
    "@types/yargs": "^17.0.32",
    "ansi-escapes": "^7.0.0",
    "ava": "^6.1.3",
    "c8": "^10.1.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "husky": "^9.1.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "mute-stream": "^1.0.0",
    "prettier": "^3.3.3",
    "semantic-release": "^24.0.0",
    "sinon": "^18.0.0",
    "strip-ansi": "^7.1.0",
    "tsimp": "^2.0.11",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  },
  "overrides": {
    "typescript-eslint": {
      "eslint": "^9.7.0"
    },
    "@typescript-eslint/eslint-plugin": {
      "eslint": "^9.7.0"
    },
    "@typescript-eslint/parser": {
      "eslint": "^9.7.0"
    },
    "@typescript-eslint/utils": {
      "eslint": "^9.7.0"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.ts": [
      "prettier --ignore-unknown --write",
      "eslint"
    ]
  },
  "snyk": true
}
