{
  "name": "order-fetcher",
  "version": "3.2.0",
  "author": "Jared Reisinger",
  "license": "ISC",
  "description": "WooCommerce order fetcher",
  "keywords": [
    "WooCommerce",
    "woocommerce",
    "orders",
    "csv"
  ],
  "homepage": "https://github.com/JaredReisinger/order-fetcher#readme",
  "repository": "github:JaredReisinger/order-fetcher",
  "type": "module",
  "files": [
    "dist/**/*.js",
    "!dist/**/*.test.js"
  ],
  "bin": {
    "orders": "./dist/stub.js",
    "order-fetcher": "./dist/stub.js"
  },
  "scripts": {
    "build": "tsc",
    "adhoc": "node ./dist/stub",
    "adhoc:build": "npm run build -- --watch",
    "clean": "rm -r dist",
    "lint": "eslint ./src",
    "test": "ava",
    "test:coverage": "c8 --all --src src --per-file --reporter lcov --reporter text npm test",
    "coverage": "npm run test:coverage",
    "codecov": "codecov",
    "release": "semantic-release",
    "snyk-protect": "snyk-protect",
    "prepare": "(is-ci || husky install) && npm run snyk-protect"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.1",
    "@snyk/protect": "^1.1187.0",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "he": "^1.2.0",
    "inquirer": "^9.2.7",
    "inquirer-autocomplete": "^0.12.2",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "lodash.get": "^4.4.2",
    "moment-timezone": "^0.5.43",
    "parse-link-header": "^2.0.0",
    "printable-characters": "^1.0.42",
    "read-pkg-up": "^10.0.0",
    "table": "^6.8.1",
    "woocommerce-api": "^1.5.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node18": "^18.2.0",
    "@types/he": "^1.2.0",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^20.4.2",
    "@types/parse-link-header": "^2.0.1",
    "@types/sinon": "^10.0.15",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ava": "^5.3.1",
    "c8": "^8.0.0",
    "codecov": "^3.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "semantic-release": "^21.0.7",
    "sinon": "^15.2.0",
    "typescript": "^5.1.6"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": "tsc"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.ts": [
      "prettier --ignore-unknown --write",
      "eslint"
    ]
  },
  "snyk": true
}
