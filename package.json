{
  "name": "order-fetcher",
  "version": "0.4.0-alpha",
  "author": "Jared Reisinger",
  "license": "ISC",
  "description": "WooCommerce order fetcher",
  "keywords": [
    "WooCommerce",
    "woocommerce",
    "orders",
    "csv"
  ],
  "homepage": "https://github.com/JaredReisinger/order-fetcher#readme",
  "repository": "github:JaredReisinger/order-fetcher",
  "bin": {
    "orders": "./lib/esm-stub.js",
    "order-fetcher": "./lib/esm-stub.js"
  },
  "scripts": {
    "test": "mocha --require esm ./test/**/*.js",
    "coverage": "nyc --extension .mjs --all --per-file --reporter lcov --reporter text npm test",
    "lint": "eslint --ext js,json,mjs lib test docs",
    "start": "node ./lib/esm-stub.js",
    "list-skus": "node ./lib/esm-stub.js --list-skus"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "commander": "2.19.0",
    "esm": "3.0.84",
    "he": "1.2.0",
    "json2csv": "4.3.1",
    "lodash.get": "4.4.2",
    "moment-timezone": "0.5.23",
    "parse-link-header": "1.0.1",
    "printable-characters": "1.0.42",
    "table": "5.1.1",
    "woocommerce-api": "1.4.2",
    "yargs": "12.0.5"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.1.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.3.2",
    "eslint-plugin-mocha": "5.2.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "husky": "1.2.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "prettier": "1.15.2",
    "pretty-quick": "1.8.0",
    "sinon": "7.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}
