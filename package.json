{
  "name": "order-fetcher",
  "version": "3.0.0",
  "author": "Jared Reisinger",
  "license": "ISC",
  "description": "WooCommerce order fetcher",
  "keywords": [
    "WooCommerce",
    "woocommerce",
    "orders",
    "csv"
  ],
  "homepage": "https://github.com/JaredReisinger/order-fetcher#readme",
  "repository": "github:JaredReisinger/order-fetcher",
  "type": "module",
  "files": [
    "dist/**/*.js",
    "!dist/**/*.test.js"
  ],
  "bin": {
    "orders": "./dist/cli.js",
    "order-fetcher": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -r dist",
    "lint": "eslint ./src",
    "test": "ava",
    "coverage": "c8 --all --src src --per-file --reporter lcov --reporter text npm test",
    "codecov": "codecov",
    "release": "semantic-release",
    "#start": "node ./lib/esm-stub.js",
    "#list-skus": "node ./lib/esm-stub.js --list-skus",
    "snyk-protect": "snyk protect",
    "prepare": "(is-ci || husky install) && npm run snyk-protect"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "chalk-template": "^0.4.0",
    "he": "^1.2.0",
    "inquirer": "^8.1.0",
    "inquirer-autocomplete": "^0.12.2",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "json2csv": "^5.0.7",
    "lodash.get": "^4.4.2",
    "moment-timezone": "^0.5.34",
    "parse-link-header": "^2.0.0",
    "printable-characters": "^1.0.42",
    "read-pkg-up": "^9.1.0",
    "snyk": "^1.990.0",
    "table": "^6.8.0",
    "woocommerce-api": "^1.5.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node18": "^1.0.1",
    "@types/he": "^1.1.2",
    "@types/inquirer": "^9.0.1",
    "@types/inquirer-autocomplete-prompt": "^1.3.5",
    "@types/json2csv": "^5.0.3",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^18.7.10",
    "@types/parse-link-header": "^2.0.0",
    "@types/sinon": "^10.0.13",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "ava": "^4.3.1",
    "c8": "^7.12.0",
    "codecov": "^3.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.4",
    "sinon": "^14.0.0",
    "typescript": "^4.7.4"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": "tsc"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
