{
  "name": "order-fetcher",
  "version": "4.0.0",
  "author": "Jared Reisinger",
  "license": "ISC",
  "description": "WooCommerce order fetcher",
  "keywords": [
    "WooCommerce",
    "woocommerce",
    "orders",
    "csv"
  ],
  "homepage": "https://github.com/JaredReisinger/order-fetcher#readme",
  "repository": "github:JaredReisinger/order-fetcher",
  "type": "module",
  "files": [
    "dist/**/*.js",
    "!dist/**/*.test.js"
  ],
  "bin": {
    "orders": "./dist/stub.js",
    "order-fetcher": "./dist/stub.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "adhoc": "node ./dist/stub",
    "adhoc:build": "npm run build -- --watch",
    "clean": "rm -r dist",
    "lint": "eslint ./src",
    "test": "ava",
    "test:coverage": "c8 --all --src src --per-file --reporter lcov --reporter text npm test",
    "coverage": "npm run test:coverage",
    "codecov": "codecov",
    "release": "semantic-release",
    "snyk-protect": "snyk-protect",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@inquirer/core": "^9.0.2",
    "@inquirer/prompts": "^5.1.2",
    "@json2csv/plainjs": "^7.0.6",
    "@snyk/protect": "^1.1291.0",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "he": "^1.2.0",
    "lodash.get": "^4.4.2",
    "moment-timezone": "^0.5.45",
    "parse-link-header": "^2.0.0",
    "printable-characters": "^1.0.42",
    "read-package-up": "^11.0.0",
    "table": "^6.8.2",
    "woocommerce-api": "^1.5.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@inquirer/testing": "^2.1.25",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/ansi-escapes": "^4.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/he": "^1.2.3",
    "@types/lodash.get": "^4.4.9",
    "@types/node": "^20.12.11",
    "@types/parse-link-header": "^2.0.3",
    "@types/sinon": "^17.0.3",
    "@types/strip-ansi": "^5.2.1",
    "@types/yargs": "^17.0.32",
    "ansi-escapes": "^7.0.0",
    "ava": "^6.1.3",
    "c8": "^9.1.0",
    "codecov": "^3.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "mute-stream": "^1.0.0",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.8",
    "sinon": "^17.0.2",
    "strip-ansi": "^7.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.16.0"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": "tsc"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.ts": [
      "prettier --ignore-unknown --write",
      "eslint"
    ]
  },
  "snyk": true
}
